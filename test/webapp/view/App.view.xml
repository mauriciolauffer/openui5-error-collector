<mvc:View controllerName="openui5.errorcollector.controller.App" displayBlock="true"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  height="100%">
  <App id="app" busy="{app>/views/app/busy}" busyIndicatorDelay="0">
